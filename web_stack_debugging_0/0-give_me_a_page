
#!/usr/bin/env bash
# Start Apache so as to return a
# page conatining Hello Holberton
# Check if Apache service is running
if systemctl is-active --quiet apache2; then
    apache_status="apache2 is running"
else
    apache_status="apache2 is not running"
fi

# Output message including Apache service status
echo "Hello Holberton"
echo "Service Status: $apache_status"

